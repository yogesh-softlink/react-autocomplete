{
  "name": "@yogesh-softlink/react-autocomplete",
  "version": "1.8.2",
  "description": "Accessible, extensible, Autocomplete for React.js",
  "main": "./build/lib/Autocomplete.js",
  "files": [
    "build/lib/index.js",
    "build/lib/Autocomplete.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yogesh-softlink/react-autocomplete.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/yogesh-softlink/react-autocomplete",
  "bugs": "https://github.com/yogesh-softlink/react-autocomplete/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run build:component",
    "build:component": "cross-env NODE_ENV=production npm run build:setup && npm run build:babel && npm run build:browserify && npm run build:uglify",
    "build:setup": "node -e \"const fs=require('fs'); fs.mkdirSync('build/lib', {recursive:true}); fs.mkdirSync('dist', {recursive:true})\"",
    "build:babel": "babel lib/index.js lib/Autocomplete.js -d build/",
    "build:browserify": "cd dist && browserify ../lib/Autocomplete.js --transform babelify --external react --external react-dom --debug | youemdee ReactAutocomplete --dependency react:React --dependency react-dom:ReactDOM | exorcist react-autocomplete.js.map > react-autocomplete.js",
    "build:uglify": "cd dist && terser react-autocomplete.js --compress --mangle -o react-autocomplete.min.js",
    "test": "npm run test:lint && npm run test:jest",
    "test:lint": "eslint lib example",
    "test:jest": "jest",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "6.1.2",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^7.0.3",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^2.13.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^6.10.3",
    "exorcist": "^0.4.0",
    "jest": "^20.0.4",
    "lodash.flow": "^3.5.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "react": "^15.5.4",
    "react-docgen": "^2.15.0",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "st": "^1.2.0",
    "terser": "^5.44.0",
    "watchify": "^3.8.0",
    "youemdee": "^1.0.0"
  },
  "tags": [
    "react",
    "autocomplete",
    "combobox",
    "a11y"
  ],
  "keywords": [
    "react",
    "autocomplete",
    "combobox",
    "accessibility"
  ],
  "dependencies": {
    "dom-scroll-into-view": "1.0.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0-0 || ^16.0.0-0",
    "react-dom": "^0.14.7 || ^15.0.0-0 || ^16.0.0-0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ]
  }
}
